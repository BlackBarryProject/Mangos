           CXXFLAGS="$CXXFLAGS -noex"
         fi

                  if (CC -V 2>&1 | $EGREP 'Compilers 4\.2' > /dev/null); then
           CXXFLAGS="$CXXFLAGS -features=castop -features=rtti"
         fi

