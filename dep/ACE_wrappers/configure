      else
        { echo "$as_me:$LINENO: WARNING: Purify program was not found." >&5
echo "$as_me: WARNING: Purify program was not found." >&2;}
        { echo "$as_me:$LINENO: WARNING: Disabling purify support." >&5
echo "$as_me: WARNING: Disabling purify support." >&2;}
      fi
      ;;
    no)
      PURELINK=""
      ;;
    *)
      { { echo "$as_me:$LINENO: error: bad value ${enableval} for --enable-purify" >&5
echo "$as_me: error: bad value ${enableval} for --enable-purify" >&2;}
   { (exit 1); exit 1; }; }
      ;;
   esac

else
  PURELINK=""
fi


