   case 16: to[15] = from[15] ;
   case 15: to[14] = from[14] ;
   case 14: to[13] = from[13] ;
   case 13: to[12] = from[12] ;
   case 12: to[11] = from[11] ;
   case 11: to[10] = from[10] ;
