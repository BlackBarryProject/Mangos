   case  6: to[5] = from[5] ;
   case  5: to[4] = from[4] ;
   case  4: to[3] = from[3] ;
   case  3: to[2] = from[2] ;
   case  2: to[1] = from[1] ;
   case  1: to[0] = from[0] ;
   case  0: return dest;
   default: return memcpy (dest, src, n);
 }
